<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.yst.web.model">
	<class name="DicRelation" table="dic_relation" dynamic-insert="true" dynamic-update="true">
		<id name="relation_id" type="integer" column="relation_id">
			<generator class="native" />
		</id>
		<property name="name" type="string" length="30" column="name" />
	</class>
</hibernate-mapping>
