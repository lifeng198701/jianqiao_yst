<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.yst.web.model">
	<class name="DicOffice" table="dic_office" dynamic-insert="true" dynamic-update="true">
		<id name="office_id" type="integer" column="office_id">
			<generator class="native" />
		</id>
	<property name="name" type="string" length="100" column="name" />
	<property name="des" type="string" length="500" column="des" />
	</class>
</hibernate-mapping>
