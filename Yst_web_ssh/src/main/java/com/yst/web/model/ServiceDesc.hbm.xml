<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.yst.web.model">
	<class name="ServiceDesc" table="service_desc" dynamic-insert="true" dynamic-update="true">
		<id name="id" type="integer" column="id">
			<generator class="native" />
		</id>
		<property name="type" type="integer" column="type" />
		<property name="image_url" length="200" column="image_url" />
		<property name="type_desc" length="500" column="type_desc"/>
	</class>
</hibernate-mapping>
